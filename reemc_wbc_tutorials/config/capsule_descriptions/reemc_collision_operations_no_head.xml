<robot name="reemc">

    <group name="right_arm_group">
        <link name="arm_right_3_link" />
        <link name="arm_right_4_link" />
        <link name="arm_right_5_link" />
        <link name="arm_right_7_link" />
    </group>
    
    <group name="left_arm_group">
        <link name="arm_left_3_link" />
        <link name="arm_left_4_link" />
        <link name="arm_left_5_link" />
        <link name="arm_left_7_link" />
    </group>

    <group name="right_leg_group">
        <link name="leg_right_3_link" />
        <link name="leg_right_4_link" />
    </group>

    <group name="left_leg_group">
        <link name="leg_left_3_link" />
        <link name="leg_left_4_link" />
    </group>
 
    <group name="torso_group">
        <link name="torso_1_link" />
        <link name="torso_2_link" />
        <link name="base_link" />
    </group>
    
    <enable_collisions_group group1="right_arm_group" group2 ="left_arm_group" />

    <enable_collisions_group group1="right_arm_group" group2 ="torso_group" />
    <enable_collisions_group group1="right_arm_group" group2 ="right_leg_group" />
    <enable_collisions_group group1="right_arm_group" group2 ="left_leg_group" />

    <enable_collisions_group group1="left_arm_group" group2 ="torso_group" />
    <enable_collisions_group group1="left_arm_group" group2 ="right_leg_group" />
    <enable_collisions_group group1="left_arm_group" group2 ="left_leg_group" />
     
 </robot>

